<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
  <extension point="com.st.tools.spc5.appbundle">
    <application name="NIL RTOS SPC560Dxx Test Application">
      <description>Test application for the SPC560Dxx NIL RTOS using the SPC5's Application Wizard.</description>
      <category name="Board">
        <description>Supported Board</description>
        <metadata value="SPC560DXX_EVB" />
      </category>
      <category name="Device">
        <description>Supported Device</description>
        <metadata value="SPC560D40L3" />
      </category>
      <category name="Drivers">
        <description>Enabled Drivers</description>
        <metadata value="PAL" />
