    </application>
    <application name="NIL RTOS SPC560Dxx Test Application for Discovery">
      <description>Test application for the SPC560Dxx created using the SPC5's Application Wizard.</description>
      <category name="Board">
        <description>Supported Board</description>
        <metadata value="SPC56D_DISCOVERY" />
      </category>
      <category name="Device">
        <description>Supported Device</description>
        <metadata value="SPC560D40L1" />
      </category>
      <category name="Drivers">
        <description>Enabled Drivers</description>
        <metadata value="PAL" />
        <metadata value="Serial" />
      </category>
      <category name="RTOS">
        <description>Supported Operating System</description>
        <metadata value="NIL" />
      </category>
    </application>
  </extension>
</plugin>
