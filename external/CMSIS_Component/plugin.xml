<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<!-- All SPC5 Component extension shall have the namespace "com.st.tools.spc5"-->
   <extension
         id="org.chibios.spc5.components.external.cmsis"
         name="CMSIS Component"
         point="com.st.tools.spc5.extension">
      <component
            hidden="true"
            isPlatform="false">
            <description>
                CMSIS files.
            </description>
            <groups
                  name="CMSIS Settings">
               <description>
                  CMSIS options and settings.
               </description></groups>
            <exportedFeatures>
               <exportedFeature
                     brief="CMSIS definitions interface."
                     exclusive="false"
                     id="com.arm.spc5.features.cmsis">
               </exportedFeature>
            </exportedFeatures>
            <requiredFeatures>
               <requiredFeature
                     id="com.st.tools.spc5.features.platform.arm.cortex.mx">
               </requiredFeature>
            </requiredFeatures>
      </component>
   </extension>

</plugin>
